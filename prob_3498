class Solution {
    public int reverseDegree(String s) {
        int index = 0, rev = 0, sum = 0;
    
        for(int i = 0;i < s.length();i++){
            char c = s.charAt(i);
            index = Character.toUpperCase(c) - 'A' + 1;
            rev = 27 - index;
            sum = sum + (rev * (i +1));
        }
        return sum;
    }
}
